author = {FAO},
file = {:D\:/Mendeley/FAO - 2014 - Manual Pr{\'{a}}ctico para el Cultivo de la Trucha Arco{\'{i}}ris.pdf:pdf},
journal = {FAO Fisheries and Aquaculture},
