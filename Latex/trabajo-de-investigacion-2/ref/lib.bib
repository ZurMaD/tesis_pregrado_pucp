	author = {{Orellana Lizano}, Henry},
	file = {:D\:/Mendeley/Orellana Lizano - 2008 - Dise{\~{n}}o y desarrollo de un algoritmo que permita estimar el tama{\~{n}}o de peces, aplicando visi{\'{o}}n por computadora, y.pdf:pdf},
